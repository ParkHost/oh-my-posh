{
	"final_space": true,
	"console_title": true,
	"console_title_style": "template",
	"console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
	"blocks": [{
			"type": "prompt",
			"alignment": "left",
			"horizontal_offset": 0,
			"vertical_offset": 0,
			"segments": [{
					"type": "root",
					"style": "diamond",
					"powerline_symbol": "",
					"invert_powerline": false,
					"foreground": "#ffff66",
					"background": "#2f2f2f",
					"leading_diamond": "",
					"trailing_diamond": "",
					"properties": {
						"postfix": "\u003c#7a7a7a\u003e ╱\u003c/\u003e",
						"root_icon": "\uF0E7"
					}
				},
				{
					"type": "path",
					"style": "plain",
					"powerline_symbol": "",
					"invert_powerline": false,
					"foreground": "#ffffff",
					"background": "#2f2f2f",
					"leading_diamond": "",
					"trailing_diamond": "",
					"properties": {
						"enable_hyperlink": true,
						"home_icon": "\uF015 ~",
						"prefix": "\u003c#f2f200\u003e \u003c/\u003e",
						"style": "full"
					}
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "",
					"foreground": "#a3a310",
					"background": "#2f2f2f",
					"properties": {
						"prefix": "\u003c#ffffff\u003e╱ \u003c/\u003e",
						"display_stash_count": true,
						"display_upstream_icon": true
					}
				}
			]

		},
		{
			"type": "prompt",
			"alignment": "right",
			"horizontal_offset": 0,
			"vertical_offset": 0,
			"segments": [{
					"type": "text",
					"style": "diamond",
					"powerline_symbol": "",
					"invert_powerline": false,
					"foreground": "#fafafa",
					"background": "#2f2f2f",
					"leading_diamond": "\uE0BA",
					"trailing_diamond": "",
					"properties": {
						"postfix": "",
						"prefix": "",
						"text": ""
					}
				},
				{
					"type": "exit",
					"style": "diamond",
					"foreground": "#008000",
					"background": "#2f2f2f",
					"leading_diamond": "",
					"trailing_diamond": "",
					"properties": {
						"display_exit_code": false,
						"always_enabled": true,
						"error_color": "#e91e63",
						"color_background": false,
						"success_icon": "\u2714 ",
						"error_icon": "\u2718 ",
						"postfix": "\u003c#ffffff\u003e╱\u003c/\u003e"
					}
				},
				{
					"type": "executiontime",
					"style": "plain",
					"powerline_symbol": "",
					"invert_powerline": false,
					"foreground": "#FFCE5C",
					"background": "#2f2f2f",
					"leading_diamond": "",
					"trailing_diamond": "",
					"properties": {
						"postfix": "\u003c#fafafa\u003e \uF252 ╱\u003c/\u003e",
						"style": "roundrock",
						"threshold": 500
					}
				},
				{
					"type": "time",
					"style": "diamond",
					"powerline_symbol": "",
					"invert_powerline": false,
					"foreground": "#fafafa",
					"background": "#2f2f2f",
					"leading_diamond": "",
					"trailing_diamond": "\uE0BC",
					"properties": {
						"postfix": "\u003c#007ACC\u003e \uF64F \u003c/\u003e",
						"time_format": "15:04:05"
					}
				}
			]
		},
		{
			"type": "newline"
		},
		{
			"type": "prompt",
			"alignment": "left",
			"horizontal_offset": 1000,
			"vertical_offset": 0,
			"segments": [{
				"type": "text",
				"style": "diamond",
				"powerline_symbol": "",
				"invert_powerline": false,
				"foreground": "#fafafa",
				"background": "#2f2f2f",
				"leading_diamond": "",
				"trailing_diamond": "\uE0BC",
				"properties": {
					"postfix": "",
					"prefix": "",
					"text": "\uE602"
				}
			}]
		}
	]
}
